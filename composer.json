{
    "name": "dne/template-manager",
    "description": "Custom Template Manager",
    "version": "1.0.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "require": {
        "shopware/core": "~v6.3.0"
    },
    "authors": [
        {
            "name": "David Neustadt"
        }
    ],
    "extra": {
        "shopware-plugin-class": "Dne\\TemplateManager\\DneTemplateManager",
        "copyright": "(c) David Neustadt",
        "label": {
            "de-DE": "Custom Template Manager",
            "en-GB": "Custom Template Manager"
        },
        "description": {
            "de-DE": "Templates in der Administration bearbeiten",
            "en-GB": "Edit templates within the administration"
        },
        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/david-neustadt.html",
            "en-GB": "https://store.shopware.com/en/david-neustadt.html"
        },
        "supportLink": {
            "de-DE": "https://davidneustadt.de",
            "en-GB": "https://neustadt.dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Dne\\TemplateManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dne\\TemplateManager\\Test\\": "tests/"
        }
    },
    "suggest": {
        "shopware/storefront": "Require '6.3'.",
        "shopware/administration": "Require '6.3'. Allows you to to set up the plugin easily"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    }
}
